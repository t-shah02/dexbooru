{
	"name": "dexbooru-migrate",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env.local -- vite dev",
		"migratedb:dev": "dotenv -e .env.local -- npx prisma migrate dev",
		"resetdb:dev": "dotenv -e .env.local -- npx prisma migrate reset",
		"deploydb:dev": "dotenv -e .env.local -- npx prisma migrate deploy",
		"deploydb:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
		"dbstudio:dev": "dotenv -e .env.local -- npx prisma studio",
		"build": "vite build",
		"preview": "dotenv -e .env.prod -- vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/markdown-it": "^12.2.3",
		"@types/markdown-it-emoji": "^2.0.2",
		"@types/probe-image-size": "^7.2.0",
		"@types/sanitize-html": "^2.8.0",
		"@types/sharp": "^0.31.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.11.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.11.0",
		"@upstash/redis": "^1.19.1",
		"bcryptjs": "^2.4.3",
		"dotenv": "^16.0.3",
		"firebase": "^9.18.0",
		"flowbite": "^1.6.3",
		"flowbite-typography": "^1.0.3",
		"imagekit": "^4.1.2",
		"markdown-it": "^13.0.1",
		"markdown-it-emoji": "^2.0.2",
		"markdown-it-named-code-blocks": "^0.1.0",
		"picmo": "^5.8.1",
		"sanitize-html": "^2.8.1",
		"sharp": "^0.31.3",
		"svelte-flatpickr": "^3.3.2"
	}
}
